{
  "name": "scry",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"pnpm:dev:next\" \"pnpm:dev:convex\"",
    "dev:next": "next dev --turbopack",
    "dev:convex": "convex dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:vercel": "node scripts/vercel-build.cjs",
    "start": "next start",
    "lint": "next lint",
    "convex:deploy": "convex deploy",
    "convex:deploy:dev": "npx convex deploy",
    "convex:deploy:prod": "CONVEX_DEPLOY_KEY=\"$CONVEX_DEPLOY_KEY_PROD\" npx convex deploy",
    "convex:codegen:prod": "CONVEX_DEPLOY_KEY=\"$CONVEX_DEPLOY_KEY_PROD\" npx convex codegen",
    "convex:init": "convex dev --once",
    "test": "vitest",
    "test:unit": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:install": "playwright install",
    "assets:test": "node scripts/create-test-assets.js",
    "assets:generate": "node scripts/generate-static-assets.js",
    "assets:generate-high": "node scripts/generate-static-assets.js --priority high",
    "assets:generate-all": "node scripts/generate-static-assets.js --verbose",
    "env:validate": "node scripts/validate-env.js",
    "env:validate:prod": "node scripts/validate-env.js --env=production",
    "env:validate:preview": "node scripts/validate-env.js --env=preview",
    "env:example": "node scripts/validate-env.js --generate-example",
    "deploy:check": "node scripts/check-deployment-readiness.js",
    "deploy:check:preview": "node scripts/check-deployment-readiness.js --env=preview",
    "convex:validate": "tsx scripts/validate-convex-deployment.ts",
    "convex:validate:prod": "tsx scripts/validate-convex-deployment.ts --prod",
    "prepare": "husky"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@formkit/auto-animate": "^0.8.2",
    "@hookform/resolvers": "^5.1.1",
    "@openrouter/ai-sdk-provider": "^0.7.2",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^4.3.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "convex": "^1.25.2",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.522.0",
    "next": "15.3.4",
    "next-themes": "^0.4.6",
    "pino": "^9.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-hotkeys-hook": "^5.1.0",
    "resend": "^4.6.0",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "ts-fsrs": "^5.2.1",
    "ws": "^8.18.3",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "bash -c 'pnpm tsc --noEmit'"
    ],
    "**/*.{test,spec}.{js,ts}": [
      "vitest related --run"
    ]
  }
}
